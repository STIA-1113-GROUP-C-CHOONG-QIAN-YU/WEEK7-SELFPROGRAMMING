package selfProgramming;

public class LoopExercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num;
		for (num=2; num<20; num++)
		{
			num = num *2;
			if (num < 15)
				System.out.println("The number is smaller than 15: " + num);
			else if (num >15)
				System.out.println("The number is greater than 15: " + num);
		}
			
	}

}
